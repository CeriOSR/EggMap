# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'eggmap3' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for eggmap3
pod 'GoogleMaps'
pod 'GooglePlaces'
pod 'Alamofire', '~> 4.7'


  target 'eggmap3Tests' do
    inherit! :search_paths
    # Pods for testing
pod 'GoogleMaps'
pod 'GooglePlaces'
pod 'Alamofire', '~> 4.7'

  end

  target 'eggmap3UITests' do
    inherit! :search_paths
    # Pods for testing
pod 'GoogleMaps'
pod 'GooglePlaces'
pod 'Alamofire', '~> 4.7'

  end

end
